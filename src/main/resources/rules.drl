import it.unicam.cs.Flexchain.BlockchainUtils;
global java.util.List names;
global java.util.List values;

dialect  "mvel"
rule "Message_1e90tfn"
when
  b : BlockchainUtils(b.getState("Message_1e90tfn")==0,  || b.getState("Message_1j4s0qh")==2)
then
    names.add("products");
    values.add(b.getSingleInput(0));
    b.setVariablesToContract(names, values, "Message_1e90tfn");
 end

