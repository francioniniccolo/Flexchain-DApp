
import it.unicam.cs.Flexchain.BlockchainUtils
global java.util.List types;
global java.util.List variables;
global java.util.List values;

dialect  "mvel"

rule "Message_1pam53q"
when
   b : BlockchainUtils(b.getState("Message_1pam53q")==0)
then
    types.add(new String[]{"string"});
    variables.add(new String[]{"receipt_id"});
    values.add(new String[]{b.getSingleInput(0)});
    b.setVarialesToContract(types, variables, values, "Message_1pam53q");
 end
