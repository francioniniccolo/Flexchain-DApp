
import it.unicam.cs.Flexchain.BlockchainUtils
import java.util.List
import java.util.Arrays

dialect  "mvel"

rule "Message_1pam53q"
when
   b : BlockchainUtils(b.getState("Message_1pam53q")==0)
then
    List<String> types = Arrays.asList(new String[]{"string"});
    List<String> variables = Arrays.asList(new String[]{"receipt_id"});
   List<String> values = Arrays.asList(new String[]{b.getSingleInput(0)});
b.setVarialesToContract(types, variables, values, "Message_1pam53q");
 end
