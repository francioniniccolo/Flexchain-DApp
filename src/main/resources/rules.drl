
import it.unicam.cs.Flexchain.BlockchainUtils;
global java.util.List names;
global java.util.List values;

dialect  "mvel"

rule "Message_1pam53q"
when
   b : BlockchainUtils(b.getState("Message_1pam53q")==0)
then
    names.add(new String[]{"receipt_id"});
    values.add(new String[]{b.getSingleInput(0)});
    b.setVarialesToContract(names, values, "Message_1pam53q");
 end
