import it.unicam.cs.Flexchain.BlockchainUtils;
global java.util.List names;
global java.util.List values;

dialect  "mvel"
rule "Message_1pam53q"
when
  b : BlockchainUtils(b.getState("Message_1pam53q")==0)
then
    names.add("pizza");
    values.add(b.getSingleInput(0));
    b.setVariablesToContract(names, values, "Message_1pam53q");
 end

rule "Message_1mi4idx"
when
  b : BlockchainUtils(b.getState("Message_1mi4idx")==0, b.getState("Message_1pam53q")==2)
then
    names.add("mess");
    values.add(b.getSingleInput(0));
    b.setVariablesToContract(names, values, "Message_1mi4idx");
 end

rule "Message_1rnq4x3"
when
  b : BlockchainUtils(b.getState("Message_1rnq4x3")==0, b.getState("Message_1mi4idx")==2)
then
    names.add("p");
    values.add(b.getSingleInput(0));
    b.setVariablesToContract(names, values, "Message_1rnq4x3");
 end

